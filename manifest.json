{
    "name": "NZBMatrixIMDb",
    "version": "0.2",
    "description": "Small Chrome extension that adds IMDb information to
NZBMatrix",
    "content_scripts": [
        {
        "matches" : ["http://nzbmatrix.com/*", "https://nzbmatrix.com/*"],
        "js": ["jquery-1.6.4.min.js","nzbmatrix-imdb.js"]
        }
    ],
    "options_page": "options.html",
    "icons": {
        "48":  "nzbmatriximdb48.png",
        "128": "nzbmatriximdb128.png" },
    "permissions": [
        "http://www.imdbapi.com/*"
    ]
}
